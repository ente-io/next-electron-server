    "files": [
      "main",
      "out"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "dependencies": {
    "next-electron-server": "latest"
  }
}
