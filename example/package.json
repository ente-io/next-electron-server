{
  "name": "next-electron-server-example",
  "productName": "ElectronNext",
  "version": "0.0.1",
  "description": "Example using next-electron-server",
  "license": "MIT",
  "main": "main/index.js",
  "scripts": {
    "dev": "concurrently -k \"next dev\" \"electron .\"",
    "build": "next build && next export && electron-builder --dir"
